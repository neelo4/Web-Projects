if (href === telLink) {
  return (
    <>
      {showNumber ? (
        <>
          {maskedNumber}
          <Link data-testid="phoneNumberLink" onClick={revealPhone}>
            {unmaskedNumber}
          </Link>
        </>
      ) : (
        <Button variant="primary" onClick={revealPhone} data-testid="phoneNumberLink">
          {maskedNumber}
          <span className="ml-2">{showNumberText}</span>
        </Button>
      )}
    </>
  );
}
