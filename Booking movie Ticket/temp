if (policy.policyCurrentStatus === PolicyStatusOrder.PaymentOverdue) {
  return (
    <PolicyNotification
      type="warning"
      label={tt(paymentOverdueHeading)}
      subtext={
        <PhoneNumber 
          content={tt(getFullBrandStringNameByUserType('overdueForRenewalHeading'))}
          pagename={PageName.DASHBOARD}
        />
      }
    />
  );
}
