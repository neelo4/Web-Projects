const getPrefixText = () => {
  if (!prefix) return '';
  if (typeof prefix === 'string') {
    return tt(getFullBrandStringNameByUserType(prefix));
  }
  return prefix;
};

const getSuffixText = () => {
  if (!suffix) return '';
  if (typeof suffix === 'string') {
    return tt(getFullBrandStringNameByUserType(suffix));
  }
  return suffix;
};
